{
  "name": "kickstart-next",
  "version": "1.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/kickstart-next"
  },
  "description": "Contentstack Kickstarts are the minimum amount of code needed to connect to Contentstack.",
  "author": "Tim Benniks <tim.benniks@contentstack.com>",
  "annotation": "Lo Etheridge <lo.etheridge@contentstack.com>",
  "homepage": "https://contentstack.com",
  "license": "MIT",
  "scripts": {
    "tsgen": "csdx tsgen -a CSDT -o \"./.generated/index.ts\" -p \"I\" --branch=main --no-doc --include-system-fields --include-editable-tags",
    "gen-config": "tsx ./scripts/generate-component-mapper.ts",
    "fetch-languages": "tsx --require dotenv-flow/config scripts/fetch-language.ts",
    "gen-config:watch": "tsx ./scripts/generate-component-mapper.ts --watch",
    "next:build": "next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "next:start": "next start",
    "dev": "cross-env NODE_ENV=development npm-run-all --serial bootstrap:run --parallel gen-config:watch next:dev",
    "build": "cross-env NODE_ENV=production npm-run-all --serial gen-config bootstrap:run next:build",
    "start": "cross-env-shell NODE_ENV=production npm-run-all --serial bootstrap:run gen-config next:build next:start",
    "lint": "next lint",
    "figma-tokens": "ts-node --require dotenv-flow/config --project tsconfig.scripts.json scripts/figma-tokens/sync-figma-to-tokens.ts",
    "theme-config": "ts-node --require dotenv-flow/config --project tsconfig.scripts.json scripts/figma-tokens/generate-css-variables.ts"
  },
  "dependencies": {
    "@contentstack/delivery-sdk": "^4.10.3",
    "@contentstack/json-rte-serializer": "^3.0.3",
    "@contentstack/live-preview-utils": "^3.4.0",
    "@contentstack/management": "^1.27.3",
    "@contentstack/utils": "^1.4.4",
    "@figma/rest-api-spec": "^0.23.0",
    "@next/third-parties": "^14.2.7",
    "@timbenniks/contentstack-endpoints": "^1.0.10",
    "autoprefixer": "^10.4.19",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "next": "^15.3.6",
    "postcss": "^8.4.38",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-singleton-hook": "^4.0.1",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "cross-env": "~7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "npm-run-all": "~4.1.5",
    "prettier": "^3.3.2",
    "react-syntax-highlighter": "^15.5.0",
    "stylelint-order": "^6.0.4",
    "tailwind-config": "^0.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom",
    "next": "$next"
  }
}